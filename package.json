{
  "name": "weave-react",
  "version": "1.0.0",
  "description": "Weave React Components",
  "main": "src/index.js",
  "repository": "git@github.com:yorthehunter/weave-react.git",
  "author": "yorthehunter <boru.design@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest",
    "coverage": "jest --watch --coverage --colors --verbose",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-es6-displayname": "^1.0.0-beta1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "shortid": "^2.2.8",
    "weave-ui": "https://github.com/yorthehunter/weave-ui"
  },
  "devDependencies": {
    "babel-jest": "^23.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "jest": "^23.2.0",
    "jest-cli": "^23.2.0",
    "jest-enzyme": "^6.0.2",
    "node-sass": "^4.9.0",
    "react-styleguidist": "^7.0.19",
    "react-test-renderer": "^16.4.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "rootDir": "./src",
    "coverageDirectory": "<rootDir>/../coverage",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/../__mocks__/fileMock.js",
      "\\.(css|scss|sass)$": "<rootDir>/../__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/../node_modules/jest-enzyme/lib/index.js",
    "setupFiles": [
      "<rootDir>/../setupTests.js"
    ]
  }
}
